<h2>Log In To Your Account</h2>
<form method="POST" action="/session">
  <label>Email Address:
    <input type="email" name="user[email]" value="<%= @login_attempt.email %>"/>
    </label>
      <%= erb :_error, { :locals => { :model => @login_attempt, :field_name =>
      :email } } %>

      <label>Password: <input type="password" name="user[password]" /></label>
        <%= erb :_error, { :locals => { :model => @login_attempt, :field_name =>
        :password } } %>

        <button>Log In</button>
</form>